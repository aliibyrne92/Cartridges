<script>
    function openAccordion(categoryBtn) {
        var openAccs = document.getElementsByClassName("openAccordion");
        var openCategories = document.getElementsByClassName("activeCategory");
        var activeCategory = categoryBtn.nextElementSibling;
        
        for(var j=0; j < openAccs.length; j++){
            if(activeCategory != openAccs[j]){
                openAccs[j].classList.replace("openAccordion","closedAccordion");
            }
        }

        for(var i=0; i < openCategories.length; i++){
            if(categoryBtn != openCategories[i]){
                openCategories[i].classList.toggle("activeCategory");
            }
        }

        categoryBtn.classList.toggle("activeCategory");
        
        if (activeCategory.classList.contains("openAccordion")) {
            activeCategory.classList.replace("openAccordion","closedAccordion");
        } else {
            activeCategory.classList.replace("closedAccordion","openAccordion");
        }
    }
</script>